{
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    },
    "cleanUrls": true,
    "trailingSlash": false,
    "public": ".next",
    "rewrites": [
      {
        "source": "**",
        "function": "ssrbhavanidigitals"
      }
    ],
    "predeploy": ["npm run build"]
  }
}
